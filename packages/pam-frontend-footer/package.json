{
  "name": "pam-frontend-footer",
  "version": "1.0.0",
  "description": "Footer for PAMs frontend-applikasjoner",
  "author": "teampam",
  "license": "MIT",
  "repository": "navikt/pam-frontend-footer",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "test": "cross-env CI=1 react-scripts-ts test --env=jsdom",
    "test:watch": "react-scripts-ts test --env=jsdom",
    "build": "rollup -c",
    "clean-build": "rimraf ./dist && mkdir dist && rollup -c",
    "start": "rollup -c -w",
    "prepare": "npm run clean-build",
    "predeploy": "cd example && npm install && npm run build",
    "deploy": "gh-pages -d example/build"
  },
  "peerDependencies": {
    "@types/react-router-dom": "^4.3.1",
    "ajv": "^6.5.4",
    "classnames": "^2.2.6",
    "nav-frontend-core": "^4.0.5",
    "react": "^15.0.0 || ^16.0.0"
  },
  "devDependencies": {
    "@types/jest": "^23.1.5",
    "@types/react": "^16.3.13",
    "ajv": "^6.5.4",
    "babel-core": "^6.26.3",
    "babel-runtime": "^6.26.0",
    "classnames": "^2.2.6",
    "cross-env": "^5.1.4",
    "gh-pages": "^1.2.0",
    "nav-frontend-core": "^4.0.5",
    "react": "^16.6.3",
    "react-scripts-ts": "^2.16.0",
    "rollup": "^0.62.0",
    "rollup-plugin-babel": "^3.0.7",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-less": "^0.1.4",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-postcss-modules": "1.0.8",
    "rollup-plugin-typescript2": "^0.13.0",
    "rollup-plugin-url": "^1.4.0",
    "typescript": "^2.8.3"
  },
  "files": [
    "dist"
  ]
}
